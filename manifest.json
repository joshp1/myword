{
    "name": "MyWord",
    "description": "This is a very simple barebones word processor. It don't have formating yet. I made it for writing at work on my android.",
    "id": "https://joshp1.github.io/myword",
    "category": ["productivity"],
    "orientation":"any",
    "short_name": "MWP",
    "start_url": "/myword/",
    "display_override": ["fullscreen", "minimal-ui"],
    "display": "standalone",
    "background_color": "#000000",
    "theme_color": "#FFFFFF",
    "icons": [
        {
            "src": "/myword/icons/icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "/icons/icon-512.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
    "launch_handler": {
        "client_mode":"auto"
    }
}
